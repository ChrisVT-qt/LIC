<lic>
  <vectorfield>
    <parameters>
      <parameter name="bla" value="0"/>
    </parameters>
    <formulas>
      <x>sin(x) + sin(2*x) + sin(3*y) - y</x>
      <y>sin(1.5*y) + sin(2.5*y) + sin(3.5*x) + x</y>
    </formulas>
  </vectorfield>
  <background type="gaussian" sigma="2"/>
  <image steps="50">
    <ranges>
      <range variable="x" min="-5" max="+5"/>
      <range variable="y" min="-5" max="+5"/>
    </ranges>
    <resolution x="1024" y="1024"/>
    <output filename="Example-4.png"/>
  </image>
</lic>
